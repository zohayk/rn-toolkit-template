{
  "name": "ProjectName",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:project": "npx react-native-clean-project",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:write": "eslint --debug app/ --fix",
    "prettier:write": "prettier --write '**/*.{js,ts,tsx}'",
    "type-check": "tsc -p tsconfig.json",
    "extract": "formatjs extract 'app/**/*.{js,jsx,ts,tsx}' --ignore='**/*.d.ts' --out-file extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile extracted/en.json --out-file app/translations/en.json",
    "code": "yarn run prettier:write && yarn run type-check && yarn run lint:write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/geolocation": "3.4.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "7.3.3",
    "@react-navigation/native": "7.0.19",
    "@react-navigation/native-stack": "7.3.3",
    "@react-navigation/stack": "7.2.3",
    "@reduxjs/toolkit": "2.6.1",
    "axios": "1.8.4",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "react": "19.1.0",
    "react-hook-form": "7.55.0",
    "react-intl": "7.1.10",
    "react-native": "0.78.1",
    "react-native-confirmation-code-field": "7.4.0",
    "react-native-date-picker": "5.0.11",
    "react-native-device-info": "14.0.4",
    "react-native-extra-dimensions-android": "1.2.5",
    "react-native-gesture-handler": "2.25.0",
    "react-native-image-picker": "8.2.0",
    "react-native-indicators": "0.17.0",
    "react-native-modal": "13.0.2",
    "react-native-permissions": "5.3.0",
    "react-native-reanimated": "3.17.1",
    "react-native-reanimated-carousel": "4.0.2",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "4.10.0",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "15.11.2",
    "react-native-webview": "13.13.5",
    "react-redux": "9.2.0",
    "redux-persist": "6.0.0",
    "use-debounce": "10.0.4",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/eslint-parser": "7.27.0",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-transform-template-literals": "7.26.8",
    "@babel/preset-env": "7.26.9",
    "@babel/runtime": "7.27.0",
    "@formatjs/cli": "6.6.3",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "17.0.0",
    "@react-native-community/cli-platform-ios": "17.0.0",
    "@react-native/babel-preset": "0.78.1",
    "@react-native/eslint-config": "0.78.1",
    "@react-native/metro-config": "0.78.1",
    "@react-native/typescript-config": "0.78.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/react": "19.0.12",
    "@types/react-native": "0.73.0",
    "@types/react-native-indicators": "0.16.6",
    "@types/react-redux": "7.1.34",
    "@types/react-test-renderer": "19.0.0",
    "babel-plugin-formatjs": "10.5.37",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "git-format-staged": "3.1.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "react-test-renderer": "19.1.0",
    "typescript": "5.8.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn run prettier:write",
      "yarn run type-check",
      "yarn run lint"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
